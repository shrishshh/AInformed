# This is more config than is usually necessary.
# If you switch to using `pnpm` in the future you should use something like:
#   https://github.com/t3-oss/create-t3-turbo/blob/main/.npmrc
legacy-peer-deps=true
strict-peer-dependencies=false
auto-install-peers=true
node-linker=hoisted
network-timeout=100000
fetch-retries=3
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
audit=false
fund=false
loglevel=error
save-exact=true
engine-strict=false 